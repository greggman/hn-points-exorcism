{
  "manifest_version": 3,
  "name": "HN Points Exorcism",
  "version": "1.0.0",
  "description": "removes the evil points from Hacker News",
  "author": "Gregg Tavares",
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://news.ycombinator.com/*"]
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/greggman/hn-points-exorcism",
  "permissions": [
  ],
  "optional_permissions": [
  ],
  "host_permissions": [
    "https://news.ycombinator.com/*"
  ]
}